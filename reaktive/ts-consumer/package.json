{
  "name": "ts-consumer",
  "version": "1.0.0",
  "description": "Consumer project to test publication",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:watch": "nodemon src/index.ts",
    "build": "tsc",
    "build:run": "yarn build && node build/index.js",
    "reinstall": "yarn remove @com.badoo/reaktive && yarn add ../build/publications/npm/js",
    "rebuild": "(cd ../../ && ./gradlew :reaktive:pack) && yarn reinstall"
  },
  "dependencies": {
    "@com.badoo/reaktive": "../build/publications/npm/js"
  },
  "devDependencies": {
    "@types/node": "^16.7.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  }
}
